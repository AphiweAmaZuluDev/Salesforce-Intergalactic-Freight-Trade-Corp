<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Estimated_Direct_Distance__c</fullName>
    <description>Calculates the straight-line distance (see Step 3).</description>
    <externalId>false</externalId>
    <formula>SQRT(
    (Destination_Planetary_System__r.X_Coordinate__c - Origin_Planetary_System__r.X_Coordinate__c) * (Destination_Planetary_System__r.X_Coordinate__c - Origin_Planetary_System__r.X_Coordinate__c) +

    (Destination_Planetary_System__r.Y_Coordinate__c - Origin_Planetary_System__r.Y_Coordinate__c) * (Destination_Planetary_System__r.Y_Coordinate__c - Origin_Planetary_System__r.Y_Coordinate__c) +

    (Destination_Planetary_System__r.Z_Coordinate__c - Origin_Planetary_System__r.Z_Coordinate__c) * (Destination_Planetary_System__r.Z_Coordinate__c - Origin_Planetary_System__r.Z_Coordinate__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Estimated Direct Distance</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
